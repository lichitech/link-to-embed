{
	"compilerOptions": {
		"target": "ES2020",
		"lib": ["ES2020", "DOM", "DOM.Iterable"],
		"baseUrl": ".",
		"module": "ESNext",
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"strict": true,
		"declaration": true,
		"declarationMap": true,
		"noEmit": true,
		"outDir": "./dist",
		"sourceMap": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true
	}
}
